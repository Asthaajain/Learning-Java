import java.util.Scanner;

class Main
{
    public static void main(String[] args)
    {
        Scanner input = new Scanner(System.in);
        
        System.out.println("Testing Code \nEnter no: ");
        
        int no = input.nextInt();
        
        System.out.println("Entered: " + no);
        
        input.close();
    }
}
